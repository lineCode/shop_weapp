<view class="shoppingcar" style="{{(minHeight)}}"><block wx:for="{{shopData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="dianpu"><block wx:if="{{item.store_name}}"><view class="dianpu-name"><check-box vue-id="{{'1-'+index}}" isselected="{{item.checked}}" data-event-opts="{{[['^change',[['shopActive',['$0'],[[['shopData','',index]]]]]]]}}" bind:change="__e" bind:__l="__l"></check-box><view class="iconfont" style="width:50rpx;height:50rpx;margin-top:-14rpx;"><image style="vertical-align:middle;" src="../../static/hudong.png" mode="scaleToFill"></image></view><view class="text">{{item.store_name}}</view></view></block><block wx:for="{{item.data}}" wx:for-item="ite" wx:for-index="ind" wx:key="ind"><scroll-view class="scrollView" scroll-x="true" id="{{ite.cart_id}}" data-index="{{ind}}" scroll-left="{{ite.scrollLeft}}" data-event-opts="{{[['touchstart',[['touchS',['$event']]]],['touchend',[['touchE',['$event']]]]]}}" bindtouchstart="__e" bindtouchend="__e"><view class="viewbox"><view class="shangpin"><check-box vue-id="{{'2-'+index+'-'+ind}}" isselected="{{ite.isChecked}}" data-event-opts="{{[['^change',[['proActive',['$0','$1'],[[['shopData','',index]],[['shopData','',index],['data','',ind]]]]]]]}}" bind:change="__e" bind:__l="__l"></check-box><view class="shangpin-info"><view class="img"><image src="{{ite.goods_image_url}}" mode="aspectFill"></image></view><view class="text-info"><view data-event-opts="{{[['tap',[['toProDetail',['$0','$1'],[[['shopData','',index],['data','',ind,'goods_id']],[['shopData','',index],['data','',ind,'store_id']]]]]]]}}" class="title-text" bindtap="__e"><text class="name">{{ite.goods_name}}</text></view><view data-event-opts="{{[['tap',[['toProDetail',['$0','$1'],[[['shopData','',index],['data','',ind,'goods_id']],[['shopData','',index],['data','',ind,'store_id']]]]]]]}}" class="jiage" bindtap="__e"><text class="danjia">{{"￥"+ite.goods_price}}</text></view><view class="numInput"><text data-event-opts="{{[['tap',[['changeCount',['$0',-1,'$1'],[[['shopData','',index],['data','',ind]],[['shopData','',index],['data','',ind,'cart_id']]]]]]]}}" class="{{['reduce iconfont',ite.goods_num==0?'numbox-disabled':'']}}" bindtap="__e">-</text><input type="number" id="{{ite.cart_id}}" data-event-opts="{{[['input',[['__set_model',['$0','goods_num','$event',[]],[[['shopData','',index],['data','',ind]]]],['inputCarCount',['$event']]]]]}}" value="{{ite.goods_num}}" bindinput="__e"/><text data-event-opts="{{[['tap',[['changeCount',['$0',1,'$1'],[[['shopData','',index],['data','',ind]],[['shopData','',index],['data','',ind,'cart_id']]]]]]]}}" class="plus iconfont" bindtap="__e">+</text></view></view></view></view><view data-event-opts="{{[['tap',[['deletePro',['$0','$1'],[[['shopData','',index],['data','',ind,'cart_id']],[['shopData','',index],['data','',ind,'store_id']]]]]]]}}" class="hong" bindtap="__e">删除</view></view></scroll-view></block></view></block><view class="bottom-jiesuan"><view class="info"><view class="allSelectText"><check-box vue-id="3" isselected="{{isCheckAll}}" data-event-opts="{{[['^change',[['allCheck']]]]}}" bind:change="__e" bind:__l="__l"></check-box><view class="allText">全选</view></view><view>总计：<text>{{"￥"+allPrice}}</text></view></view><view data-event-opts="{{[['tap',[['jiesuan',['$event']]]]]}}" class="btn" bindtap="__e">结算</view></view></view>