<view><view class="header-bar"><uni-nav-bar vue-id="1" status-bar="{{true}}" fixed="{{true}}" color="#333333" shadow="{{false}}" background-color="#0177BF" bind:__l="__l" vue-slots="{{['default','left']}}"><view slot="left"><view class="city"><tui-icon vue-id="{{('2')+','+('1')}}" name="position" color="#ffffff" size="20" bind:__l="__l"></tui-icon><view class="location-text">{{city}}</view></view></view><view class="input-view"><tui-icon vue-id="{{('3')+','+('1')}}" name="search" color="#666666" size="20" bind:__l="__l"></tui-icon><input class="input" confirm-type="search" type="text" placeholder="搜索" data-event-opts="{{[['confirm',[['confirm',['$event']]]]]}}" bindconfirm="__e"/></view></uni-nav-bar></view><view><scroll-view class="tab-view" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><block wx:for="{{tabbar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['tab-bar-item',currentTab==index?'active':'']}}" data-current="{{index}}" data-event-opts="{{[['tap',[['swichNav',['$event']]]]]}}" catchtap="__e"><text class="tab-bar-title">{{item}}</text></view></block></scroll-view><view><scroll-view class="scoll-y" scroll-y><view class="list-view"><view class="zhanwei"></view><view class="goods-list"><view class="product-list"><block wx:for="{{storeList}}" wx:for-item="store" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['toStore',['$0'],[[['storeList','id',store.id,'id']]]]]]]}}" class="product" bindtap="__e"><image mode="aspectFill" src="{{store.img}}"></image><view class="name">{{store.name}}</view><view class="info"><view class="price">{{store.price}}</view><view class="slogan">{{store.slogan}}</view></view></view></block></view></view></view></scroll-view></view></view></view>