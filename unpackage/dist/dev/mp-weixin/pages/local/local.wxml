<view><view class="header-bar"><uni-nav-bar vue-id="1" status-bar="{{true}}" fixed="{{true}}" color="#333333" shadow="{{false}}" background-color="#0177BF" bind:__l="__l" vue-slots="{{['default','left']}}"><view slot="left"><view class="city"><uni-icon vue-id="{{('2')+','+('1')}}" type="location-filled" color="#ffffff" size="18" bind:__l="__l"></uni-icon><view class="location-text">{{city}}</view></view></view><view class="input-view"><uni-icon vue-id="{{('3')+','+('1')}}" type="search" size="22" color="#666666" bind:__l="__l"></uni-icon><input class="input" confirm-type="search" type="text" placeholder="搜索" data-event-opts="{{[['confirm',[['confirm',['$event']]]]]}}" bindconfirm="__e"/></view></uni-nav-bar></view><view><scroll-view class="tab-view" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><block wx:for="{{tabbar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['tab-bar-item',currentTab==index?'active':'']}}" data-current="{{index}}" data-event-opts="{{[['tap',[['swichNav',['$event']]]]]}}" catchtap="__e"><text class="tab-bar-title">{{item}}</text></view></block></scroll-view><view><scroll-view class="scoll-y" scroll-y><view class="list-view"><view class="zhanwei"></view><view class="goods-list"><view class="product-list"><block wx:for="{{productList}}" wx:for-item="product" wx:for-index="__i0__" wx:key="goods_id"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','goods_id',product.goods_id]]]]]]]}}" class="product" bindtap="__e"><image mode="aspectFill" src="{{product.img}}"></image><view class="name">{{product.name}}</view><view class="info"><view class="price">{{product.price}}</view><view class="slogan">{{product.slogan}}</view></view></view></block></view></view></view></scroll-view></view></view></view>