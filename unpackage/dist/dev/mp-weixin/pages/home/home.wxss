}
.swiper .swiper-box {
    width: 700rpx;
    height: 233rpx;
    margin-top: 18rpx;
    border-radius: 20rpx;
    overflow: hidden;
    position: relative;
    z-index: 1;
}
.swiper .swiper-box swiper {
      width: 100%;
      height: 233rpx;
}
.swiper .swiper-box swiper swiper-item image {
        width: 100%;
