::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.tab-view::before {
  content: '';
  position: absolute;
  border-bottom: 1rpx solid #eaeef1;
  -webkit-transform: scaleY(0.5);
  -ms-transform: scaleY(0.5);
      transform: scaleY(0.5);
  bottom: 0;
  right: 0;
  left: 0;
}
.tab-view {
  width: 100%;
  height: 70rpx;
  overflow: hidden;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  position: fixed;
  top: 25px44;
  left: 0;
  z-index: 99;
  background: #fff;
  white-space: nowrap;
}
