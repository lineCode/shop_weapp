      height: 5rpx;
      border-radius: 3rpx;
      overflow: hidden;
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
}
.swiper .swiper-box .indicator .dots {
        width: 0rpx;
        background-color: white;
        -webkit-transition: all 0.3s ease-out;
        -o-transition: all 0.3s ease-out;
        transition: all 0.3s ease-out;
}
.swiper .swiper-box .indicator .dots.on {
          width: 33.33333%;
}
