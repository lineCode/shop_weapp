.first .img-video .box {
  display: inline-block;
  width: calc((84.6vw - 50rpx)/4);
  height: calc((84.6vw - 50rpx)/4);
  margin: 5rpx 5rpx;
}
.first .img-video .box image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}
.yingye {
  background: #FFFFFF;
  height: 130rpx;
  border-bottom: 2rpx solid #EBEEF5;
}
.yingye .container {
    width: 700rpx;
    margin: auto;
    padding: 14rpx 0;
}
.yingye .container .shijian {
      font-weight: 500;
      font-size: 34rpx;
}
.yingye .container .fuli {
      height: 60rpx;
      color: #409EFF;
}
.yingye .container .fuli text {
        line-height: 60rpx;
        margin-right: 10rpx;
}
.location {
  background: #FFFFFF;
  height: 80rpx;
  border-bottom: 2rpx solid #EBEEF5;
}
.go-store {
  background: #FFFFFF;
  height: 150rpx;
  border-bottom: 2rpx solid #DCDFE6;
}
.go-store .right-part {
    width: 700rpx;
    margin: auto;
    height: 150rpx;
    padding: 14rpx 0;
}
.go-store .right-part .store-name {
      font-weight: 500;
      font-size: 36rpx;
}
.go-store .right-part .store-num {
      height: 70rpx;
      font-size: 30rpx;
      display: inline-block;
      vertical-align: middle;
}
.go-store .right-part .store-num text {
        line-height: 70rpx;
}
.go-store .right-part .go-store-btn {
      float: right;
      margin-top: 4rpx;
      margin-right: 40rpx;
      background: #409eff;
}
.store-banner image {
  width: 750rpx;
  height: 375rpx;
}

