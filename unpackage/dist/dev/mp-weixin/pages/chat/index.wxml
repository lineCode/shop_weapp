<view class="container"><block wx:for="{{msgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><tui-list-cell vue-id="{{'1-'+index}}" last="{{true}}" data-event-opts="{{[['^click',[['detail']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-msg-box"><image class="tui-msg-pic" src="{{'/static/images/news/'+item.pic+'.jpg'}}" mode="widthFix"></image><view class="tui-msg-item"><view class="tui-msg-name">{{item.nickname}}</view><view class="tui-msg-content">{{item.msg}}</view></view></view><view class="{{['tui-msg-right',item.level==3?'tui-right-dot':'']}}"><view class="tui-msg-time">{{item.time}}</view><block wx:if="{{item.msgNum>0}}"><tui-badge vue-id="{{('2-'+index)+','+('1-'+index)}}" type="{{item.level==2?'gray':'danger'}}" dot="{{item.level==3?true:false}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.level!=3?item.msgNum:""}}</tui-badge></block></view></tui-list-cell></block></block></view>